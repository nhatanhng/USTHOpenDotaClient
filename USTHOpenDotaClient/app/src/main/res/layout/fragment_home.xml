<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/home_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FFFFFF">

    <TextView
        android:id="@+id/text1"
        android:layout_width="124dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="67dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="17dp"
        android:text="Home"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:gravity="top"
        />

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="385dp"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="66dp"
        android:background="@color/dark_blue" />
    <TextView
        android:id="@+id/text2"
        android:layout_width="240dp"
        android:layout_height="45dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="106dp"
        android:text="OPENDOTA"
        android:textColor="@color/white"
        android:textSize="40dp"
        android:gravity="top"
        />
    <TextView
        android:id="@+id/text3"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="166dp"
        android:text="Open source Dota 2 data platform"
        android:textColor="@color/white"
        android:textSize="19dp"
        android:gravity="top"
        />
    <RelativeLayout
        android:id="@+id/frame_1"
        android:layout_width="352dp"
        android:layout_height="39dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="206dp"
        android:clipToOutline="true"
        android:background="@drawable/my_border">

        <ImageView
            android:id="@+id/login_1"
            android:layout_width="43dp"
            android:layout_height="36dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="3dp"
            android:layout_marginRight="299dp"
            android:layout_marginBottom="0dp"
            android:src="@drawable/login_icon" />

        <TextView
            android:id="@+id/text4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="59dp"
            android:layout_marginTop="4dp"
            android:layout_marginRight="-59dp"
            android:layout_marginBottom="-2dp"
            android:gravity="top"
            android:text="Login for automatic replay parsing"
            android:textColor="@color/white"
            android:textSize="18dp" />
    </RelativeLayout>

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="190dp"
        android:layout_height="170dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="273dp"
        android:background="@color/dark_blue" />

    <ImageView
        android:id="@+id/open_source"
        android:layout_width="65dp"
        android:layout_height="67dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="72dp"
        android:layout_marginTop="273dp"
        android:src="@drawable/open_source_icon" />

    <TextView
        android:id="@+id/text5"
        android:layout_width="129dp"
        android:layout_height="50dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="340dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="22dp"
        android:gravity="top"
        android:text="Open Source"
        android:textColor="@color/light_blue"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/text6"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="26dp"
        android:layout_marginTop="368dp"
        android:gravity="top"
        android:text="All project code is open source and available for contributors to improve and modify."
        android:textColor="@color/white"
        android:textSize="15dp" />

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="190dp"
        android:layout_height="170dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="198dp"
        android:layout_marginTop="273dp"
        android:background="@color/dark_blue" />

    <ImageView
        android:id="@+id/data_chart"
        android:layout_width="65dp"
        android:layout_height="67dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="272dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/data_chart_icon" />
    <TextView
        android:id="@+id/text7"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="235dp"
        android:layout_marginTop="340dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="22dp"
        android:gravity="top"
        android:text="In_depth Data"
        android:textColor="@color/light_blue"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/text8"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="226dp"
        android:layout_marginTop="372dp"
        android:gravity="top"
        android:text="Parsing replay files provides highly detailed match data."
        android:textColor="@color/white"
        android:textSize="15dp" />

    <View
        android:id="@+id/rectangle_5"
        android:layout_width="385dp"
        android:layout_height="200dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="446dp"
        android:background="@color/dark_blue" />

    <ImageView
        android:id="@+id/magic"
        android:layout_width="97dp"
        android:layout_height="67dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="156dp"
        android:layout_marginTop="455dp"
        android:src="@drawable/magic_icon" />

    <TextView
        android:id="@+id/text9"
        android:layout_width="200dp"
        android:layout_height="34dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="518dp"
        android:gravity="top"
        android:text="Free of Charge"
        android:textColor="@color/light_blue"
        android:textSize="24dp" />

    <TextView
        android:layout_width="370dp"
        android:layout_height="150dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="546dp"
        android:text="Server are funded by sponsors and volunteers maintain the code, so the service is offered free of charge."
        android:textColor="@color/white"
        android:textSize="18dp" />


</RelativeLayout>